[ -e ../src/main.cpp ] && echo "path exists"
[ ../src/main.cpp ] && echo "path exists"
[../src/main.cpp] && echo "path exists"
[ -e ../src/mains.cpp ] || echo "path does not exist"
[ -e ../srrc ] || echo "path does not exist"
[ -f ../src/main.cpp ] && echo "path is a regular file"
[ -f ../src ] || echo "path does not exist or path is not a regular file"
[ -f ../src/mains.cpp ] || echo "path does not exist or path is not a regular file"
[ -d ../src ] && echo "path is a directory"
[ -d ../srrc ] || echo "path does not exist or path is not a directory"
[ -d ../src/main.cpp ] || echo "path does not exist or path is not a directory"
exit
